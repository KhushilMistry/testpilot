{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-flow"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-function-bind"
  ],
  "env": {
    "test": {
      "plugins": [
        "istanbul",
        "@babel/transform-flow-strip-types"
      ]
    }
  }
}
